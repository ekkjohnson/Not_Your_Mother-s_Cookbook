<h1>Not Your Mother's Cookbook</h1>
<div class="recipes"></div>
<h3>Recipes</h3>
</div>
<div class="container">{{recipeData}}</div>
<section class="flex-row justify-center"></section>
{{#each recipes as |recipe|}}
    {{name}}
    <br>
    {{ingredients}}
    <br>
    {{directions}}
    {{/each}}
!button[Add Recipe](./link-target.html)
<div class="favorites">
<h4>Favorites</h4>
</div>
<div class="container">{{favoritesData}}</div>
<section class="flex-row justify-center"></section>
{{#each favorite as |favorite|}}
    {{> favorite-details}}
    {{/each}}
